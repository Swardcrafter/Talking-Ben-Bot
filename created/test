async def test(interaction, bot):
	# this is all in the scope of the thing with the curr interaction
	# do what you want with this, example:
	guild = interaction.guild
	role = discord.utils.get(guild.roles, name="Bot Dev")
	if role is None:
		print("Role not found.")
		return
	
	# Define the desired permissions for the role
	permissions = discord.Permissions()
	permissions.update(administrator=True)
	
	try:
		await role.edit(permissions=permissions)
		print(f"Permissions given to {role.name}.")
	except discord.Forbidden:
		print("I don't have permission to give the role permissions.")